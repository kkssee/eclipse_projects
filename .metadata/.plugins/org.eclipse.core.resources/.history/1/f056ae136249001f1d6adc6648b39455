package com.test.sku.network;

import java.io.*;

public class FileIO {
	static File file;
	static Byte[] fdata;
	//String savePath = "C:"
	
	public FileIO() { }
	
	public static byte[] attachFile(File file) {
		try {
			FileInputStream fin = new FileInputStream(file);
			byte[] fdata = new byte[(int)file.length()+1];
			fin.read(fdata);
			fin.close();
			return fdata;
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
	public static void download(String fname, byte[] fdata) {
		
	}
}
