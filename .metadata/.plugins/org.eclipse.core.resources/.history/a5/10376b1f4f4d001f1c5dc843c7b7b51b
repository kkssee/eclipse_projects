package com.test.sku.pds;

import java.text.SimpleDateFormat;
import java.util.List;

public class PDSResponse {
	String msg;
	List<PDSVO> pList;
	PDSVO pv;

	public PDSResponse() { }
	public PDSResponse(String msg) {
		this.msg = msg;
	}
	@Override
	public String toString() {
		SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd");
		String sdate = sdf.format(pv.date);
		String s = String.format("\n%d\t%-15s\t%s\t%d\t%s\t%s", pv.no, pv.fname, pv.author, pv.flen, sdate, pv.desc);	
		return s;
	}
}
