package com.test.sku.network;

import java.io.ObjectInputStream;

public class ReadMsg extends Thread {
	ObjectInputStream ois;
	ChatMsg cm;
	String msg;
	
	public ReadMsg(ObjectInputStream ois, ChatMsg cm, String msg) {
		this.ois = ois;
		this.cm = cm;
		this.msg = msg;
	}

	@Override
	public void run() {
		try {
			cm = (ChatMsg)ois.readObject();
			System.out.println(cm.msg);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
