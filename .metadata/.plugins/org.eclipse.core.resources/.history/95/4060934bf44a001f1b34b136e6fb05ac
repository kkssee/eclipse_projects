package com.test.sku.serialization;

import java.io.*;
import java.util.*;

public class FileVO implements Serializable {
	String menu;
	
	String fname;
	byte[] fdata;
	String fdesc;
	String uid;
	
	List<FileVO> flist = new ArrayList<>();
	
	boolean upload;
	boolean list;
	boolean find;
	boolean update;
	boolean delete;
	boolean exit;
	
	public FileVO() { }
	public FileVO(String fname, byte[] fdata) {
		this.fname = fname;
		this.fdata = fdata;
	}
	public FileVO(String menu) {
		this.menu = menu;
	}
	
	/*@Override
	public String toString() {
		String s = String.format("%s", fname);
		return s;
	}*/
}
