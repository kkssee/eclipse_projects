package com.test.sku.Cereal;

import java.io.*;
import java.util.*;

public class DocVO implements Serializable{
	String menu;
	
	String fname;
	byte[] fdata;
	String fdesc;
	String author;
	
	List<DocVO> flist = new ArrayList<>();
	
	boolean upload;
	boolean list;
	boolean find;
	boolean update;
	boolean delete;
	boolean exit;
	
	public DocVO() { }
	public DocVO(String fname, byte[] fdata, String author, String fdesc) {
		this.fname = fname;
		this.fdata = fdata;
		this.author = author;
		this.fdesc = fdesc;
	}
	public DocVO(String menu) {
		this.menu = menu;
	}
	
	@Override
	public String toString() {
		String s = String.format("%s\t%s\t%s", fname, fdesc, author);
		return s;
	}
}
