package com.test.sku.superpet;

import java.util.ArrayList;
import java.util.List;

import com.test.sku.inheritance.Cat;
import com.test.sku.inheritance.Dog;
import com.test.sku.inheritance.Hamster;
import com.test.sku.inheritance.Pet;
import com.test.sku.inheritance.Snake;

public class SuperPetMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	private static void pets() {
		List<Pet> cart = new ArrayList<>();
		Cat cat1 = new Cat(170000, "Persian", 12, "stripped");
		Cat cat2 = new Cat(520000, "Sphynx", 8, "Hairless");
		Cat cat3 = new Cat(430000, "Maine Coon", 3, "Plain");
		Dog dog1 = new Dog(50000, "Shepherd", 7, 28.6F);
		Snake snake1 = new Snake(1100000, "Anaconda", 50.5F, "Spotted");
		Hamster hamster1 = new Hamster(15000, "Golden Hamster", 2.2F);
		
		cart.add(cat1);
		cart.add(cat2);
		cart.add(dog1);
		cart.add(snake1);
		cart.add(hamster1);
		
		System.out.println("This is your pets !");
		System.out.println("--------------------------------------------------------");
		
		for(int i = 0; i < cart.size(); i++) {
			System.out.println(cart.get(i));
		}
		
	}
}
