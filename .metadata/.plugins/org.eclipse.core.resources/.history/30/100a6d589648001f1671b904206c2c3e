package com.test.sku.network;

import java.net.Socket;

public class ReadMsg extends Thread {
	Socket s;
	ChatMsg cm;
	String msg;
	
	public ReadMsg(Socket s, ChatMsg cm, String msg) {
		this.s = s;
		this.cm = cm;
		this.msg = msg;
	}

	@Override
	public void run() {
		
		cm = (ChatMsg)ois.readObject();
		System.out.println(cm.msg);
	}
}
