package com.test.sku.pds;

import java.io.Serializable;
import java.util.List;

public class PDSVO implements Serializable, Comparable<PDSVO> {
	int no;
	long flen;
	String fname;
	String author;
	String desc;
	byte[] fdata;
	
	List<PDSVO> pList;
	PDSVO pv;
	
	public PDSVO() { }
	public PDSVO(int no) {
		super();
		this.no = no;
	}
	
	/*@Override
	public String toString() {
		String s = String.format("%s", msg);
		return s;
	}*/
	@Override
	public int compareTo(PDSVO other) {
		if(this.getNo()>other.getNo())	return 1;
		else if(this.getNo()==other.getNo()) return 0;
		else return -1;
	}
	
	
	public int getNo() {
		return no;
	}
	public long getFlen() {
		return flen;
	}
	public String getFname() {
		return fname;
	}
	public String getAuthor() {
		return author;
	}
	public String getDesc() {
		return desc;
	}
	public byte[] getFdata() {
		return fdata;
	}
	public List<PDSVO> getpList() {
		return pList;
	}
	public PDSVO getPv() {
		return pv;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public void setFlen(long flen) {
		this.flen = flen;
	}
	public void setFname(String fname) {
		this.fname = fname;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	public void setFdata(byte[] fdata) {
		this.fdata = fdata;
	}
	public void setpList(List<PDSVO> pList) {
		this.pList = pList;
	}
	public void setPv(PDSVO pv) {
		this.pv = pv;
	}
}
