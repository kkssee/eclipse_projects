package com.test.sku.serialization;

import java.io.*;
import java.net.*;

public class menuThread extends Thread {
	Socket s;
	private ObjectInputStream ois;
	private ObjectOutputStream oos;
	
	public menuThread() { }
	public menuThread(Socket s) { 
		this.s = s;
	}
	
	@Override
	public void run() {
		OutputStream out;
		
		try {
			out = s.getOutputStream();
			oos = new ObjectOutputStream(out);
			FileVO fv = new FileVO(fname, fdata);
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
	
	
}
