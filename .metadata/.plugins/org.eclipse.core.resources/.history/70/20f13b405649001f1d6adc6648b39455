package com.test.sku.network;

import java.io.*;

public class FileIO {
	static String fname;
	static Byte[] fdata;
	
	public FileIO() { }
	
	public static byte[] attachFile(String fname) {
		try {
			FileInputStream fin = new FileInputStream(fname);
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}

		return null;
	}
}
