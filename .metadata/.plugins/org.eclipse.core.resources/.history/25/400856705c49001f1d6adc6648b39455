package com.test.sku.network;

import java.io.*;

public class FileIO {
	static File fname;
	static Byte[] fdata;
	
	public FileIO() { }
	
	public static byte[] attachFile(File fname) {
		try {
			FileInputStream fin = new FileInputStream(fname);
			byte[] fdata = new byte[fin.read()];
			fin.close();
			return fdata;
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
}
